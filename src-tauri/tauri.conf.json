{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "sreal",
  "version": "0.1.0",
  "identifier": "com.sreal.media",
  "build": {
    "frontendDist": "../.output/public",
    "devUrl": "http://localhost:3001",
    "beforeDevCommand": "bun localdev",
    "beforeBuildCommand": "bun run build"
  },
  "app": {
    "macOSPrivateApi": true,
    "security": {
      "csp": null,
      "headers": {
        "Access-Control-Allow-Headers": "*"
      }
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
